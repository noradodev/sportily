<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="#F68934"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/backImageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@tools:sample/avatars"
            app:srcCompat="@drawable/baseline_arrow_back_24"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="18dp" />

        <TextView
            android:id="@+id/checkoutTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins"
            android:text="Setting"
            android:textColor="@color/white"
            android:textSize="16sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="54dp"
            tools:layout_editor_absoluteY="21dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout4"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp">

        <ImageView
            android:id="@+id/profileImageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@tools:sample/avatars"
            app:srcCompat="@drawable/user"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp" />

        <ImageView
            android:id="@+id/editImageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@tools:sample/avatars"
            app:srcCompat="@drawable/edit"
            tools:layout_editor_absoluteX="237dp"
            tools:layout_editor_absoluteY="95dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout5"
        android:layout_width="match_parent"
        android:layout_height="400dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout4"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/nameTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins"
                android:paddingLeft="20dp"
                android:text="Name"
                android:textSize="16sp" />

            <EditText
                android:id="@+id/nameEditTxt"
                android:layout_width="371dp"
                android:layout_height="48dp"
                android:layout_gravity="center"
                android:hint="@string/name_txt" />

            <TextView
                android:id="@+id/phoneTextView"
                android:layout_width="match_parent"
                android:layout_height="37dp"
                android:fontFamily="@font/poppins"
                android:paddingLeft="20dp"
                android:paddingTop="15dp"
                android:text="Phone"
                android:textSize="16sp" />

            <EditText
                android:id="@+id/phoneEditTxt"
                android:layout_width="371dp"
                android:layout_height="48dp"
                android:layout_gravity="center"
                android:hint="@string/phone_txt" />

            <TextView
                android:id="@+id/emailTextView"
                android:layout_width="match_parent"
                android:layout_height="37dp"
                android:fontFamily="@font/poppins"
                android:paddingLeft="20dp"
                android:paddingTop="15dp"
                android:text="Email"
                android:textSize="16sp" />

            <EditText
                android:id="@+id/emailEditTxt"
                android:layout_width="371dp"
                android:layout_height="48dp"
                android:layout_gravity="center"
                android:hint="@string/email_txt" />

            <TextView
                android:id="@+id/genderTextView"
                android:layout_width="match_parent"
                android:layout_height="37dp"
                android:fontFamily="@font/poppins"
                android:paddingLeft="20dp"
                android:paddingTop="15dp"
                android:text="Gender"
                android:textSize="16sp" />

            <EditText
                android:layout_width="371dp"
                android:layout_height="48dp"
                android:layout_gravity="center"
                android:hint="@string/gender_txt" />


        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="140dp"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout5">

        <Button
            android:id="@+id/bPassword"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:background="@drawable/my_btn_background"
            android:fontFamily="@font/poppins"
            android:text="Change Password"
            android:textColor="@color/black"
            android:textSize="16sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="104dp"
            tools:layout_editor_absoluteY="7dp" />

        <Button
            android:id="@+id/bLogout"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="105dp"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="106dp"
            android:layout_marginBottom="28dp"
            android:background="@drawable/btn_radius_addtocart"
            android:fontFamily="@font/poppins"
            android:text="Logout"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bPassword" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>